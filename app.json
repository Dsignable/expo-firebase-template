{
  "expo": {
    "name": "Expo Firebase Template",
    "slug": "expo-firebase-template",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "your-eas-project-id"
      },
      
      "FIREBASE_API_KEY": "POPULATED_FROM_ENV_FILE",
      "FIREBASE_AUTH_DOMAIN": "POPULATED_FROM_ENV_FILE",
      "FIREBASE_PROJECT_ID": "POPULATED_FROM_ENV_FILE",
      "FIREBASE_STORAGE_BUCKET": "POPULATED_FROM_ENV_FILE",
      "FIREBASE_MESSAGING_SENDER_ID": "POPULATED_FROM_ENV_FILE", 
      "FIREBASE_APP_ID": "POPULATED_FROM_ENV_FILE",
      "FIREBASE_MEASUREMENT_ID": "POPULATED_FROM_ENV_FILE"
    }
  }
} 